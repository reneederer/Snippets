<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="ProgramData" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="ProgramData" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xs:complexType>
      <xs:sequence minOccurs="1" maxOccurs="1">
        <xs:element name="Configuration">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="DefaultWorkingDirectory" type="xs:string" minOccurs="0" maxOccurs="1" />
              <xs:element name="Interpreter" minOccurs="1" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:attribute name="default" type="xs:boolean" />
                  <xs:attribute name="name" type="xs:string" />
                  <xs:attribute name="path" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Snippet" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Category" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
              <xs:element name="Description" type="xs:string" minOccurs="1" msdata:Ordinal="1" />
              <xs:element name="CommandText" type="xs:string" minOccurs="1" msdata:Ordinal="2" />
            </xs:sequence>
            <xs:attribute name="workingDirectory" type="xs:string" />
            <xs:attribute name="Interpreter" type="xs:string" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>